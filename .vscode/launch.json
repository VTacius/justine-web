{
    "version": "0.2.0",
    "configurations": [
        {
          "type": "chrome",
          "request": "launch",
          "name": "vuejs: chrome",
          "url": "http://127.0.0.1:4000/#/",
          "webRoot": "${workspaceFolder}/build",
          "breakOnLoad": true,
          "sourceMapPathOverrides": {
            "webpack:///app/*": "${webRoot}/*"
          }
        }
    ]
      
}
